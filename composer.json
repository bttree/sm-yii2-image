{
    "name": "bttree/sm-yii2-image",
    "description": "SiteMaket yii2 image",
    "type": "yii2-behavior",
    "keywords": ["yii2", "image", "thumbnail", "upload"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "SiteMaket team",
            "email": "dev@sitemaket.ru"
        }
    ],
    "require": {
        "imagine/imagine": "*",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "bttree/sm-yii2-upload": "*"
